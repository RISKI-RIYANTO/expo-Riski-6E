{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "types": ["expo-router/types"]
  },
  "include": ["app", "components", "data", "*.ts", "*.tsx"],
  "exclude": ["node_modules"]
}
